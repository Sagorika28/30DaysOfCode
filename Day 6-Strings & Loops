#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include<cstring>
using namespace std;

void printEO(string str) {
    int i;
    for(i = 0 ; str[i] != '\0' ; i++) {
        if(i % 2 == 0) {
            cout<<str[i];
        } 
    }
    cout<<" ";
    for(i = 0 ; str[i] != '\0' ; i++) {
        if(i % 2 != 0) {
            cout<<str[i];
        } 
    }
}

int main() {
    int t;
    cin>>t;
    while(t) {
        string str;
        cin>>str;
        printEO(str);
        cout<<endl;
        t--;
    }
    return 0;
}
