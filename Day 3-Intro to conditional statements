#include <bits/stdc++.h>

using namespace std;

void choose(int n) {
    if(n % 2 != 0) {
        cout<<"Weird";
    }
    else {
        if(n >=2 && n<=5) {
            cout<<"Not Weird";
        }
        else if(n >=6 && n<=20) {
            cout<<"Weird";
        }
        else {
            cout<<"Not Weird";
        }
    }
}


int main()
{
    int N;
    cin >> N;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');
    choose(N);
    return 0;
}
